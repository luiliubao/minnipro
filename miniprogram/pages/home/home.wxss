swiper {
  height: 400rpx;
  width: 100%;
}

swiper-item {
  height: 100%;
  width: 100%;
}

.slide-image {
  height: 100%;
  width: 100%;
}

.menu_container {
  display: flex;
  flex-wrap: wrap;
  justify-content: center;
  width: 100%;
  background-color: white;
}

.menu_item {
  width: 30%;
  display: flex;
  flex-direction: column;
  justify-content: center;
  align-items: center;
  font-size: 14px;
  box-sizing: border-box;
  padding-bottom: 10px;
  padding-top: 10px;
}

.menu_icon {
  width: 20px;
  height: 20px;
  padding: 15px;
}

.new-icon-circle {
  display: flex;
  margin: auto;
  width: 50px;
  height: 50px;
  border-radius: 25px;
  text-align: center;
  color: #fff;
}

.circle-1 {
  background-color: #FF3399;
}

.circle-2 {
  background-color: #53c7ec;
}

.circle-3 {
  background-color: #8a93f7;
}

.circle-4 {
  background-color: #fdcf43;

}

.circle-5 {
  background-color: #19be6b;

}

.circle-6 {
  background-color: #FF9933;

}